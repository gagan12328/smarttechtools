{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev:allTools": "PAGE=allTools rollup -c -w",
    "dev:tool": "PAGE=tool rollup -c -w",
    "start": "sirv public --no-clear",
    "fix": "npx eslint --fix \"{,!(node_modules|public)/**/}*.{js,svelte}\"",
    "format": "npx prettier --write \"{,!(node_modules|public)/**/}*.{js,svelte}\"",
    "export": "node ./scripts/export-script"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/node": "^7.16.8",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.8.0",
    "eslint-plugin-svelte3": "^3.4.0",
    "express": "^4.17.2",
    "fs-extra": "^10.0.0",
    "http-proxy-middleware": "^2.0.3",
    "postcss": "^8.4.5",
    "posthtml": "^0.16.5",
    "posthtml-beautify": "^0.7.0",
    "posthtml-insert-at": "^0.2.7",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.6.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-svelte-static-html": "^0.1.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^4.10.1",
    "tailwindcss": "^3.0.13",
    "vm": "^0.1.0"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "lodash.camelcase": "^4.3.0",
    "sirv-cli": "^2.0.0"
  }
}
